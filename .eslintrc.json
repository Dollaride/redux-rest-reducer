{
  "parser": "babel-eslint",
  "globals": {
    // Put things like jQuery, etc
    "jQuery": true,
    "$": true,
    "describe": true,
    "it": true
  },
  "extends": "airbnb",
  "rules": {
    "no-multiple-empty-lines": "off",
    "semi": 0,
    "import/no-unresolved": 0,
    "new-cap": 0
  }

}
